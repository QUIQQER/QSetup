{
    "name" : "quiqqer\/qsetup",
    "type" : "quiqqer-library",
    "description" : "Load essential packages for quiqqer setup",

    "version" : "dev-dev",
    "license" : "",

    "minimum-stability": "dev",

    "authors" : [{
        "name": "PCSG OHG",
        "email": "support@pcsg.de",
        "homepage": "http://www.pcsg.de",
        "role": "Developer"
    }],

    "support" : {
        "email" : "support@pcsg.de"
    },

    "repositories" : [{
        "type" : "composer",
        "url"  : "https://update.quiqqer.com/"
    }],

    "require": {
        "quiqqer\/utils"   : "dev-dev",
        "quiqqer\/qui-php" : "dev-dev"
    },

    "autoload": {
        "psr-0" : {
            "QUI" : "lib/"
        }
    },

    "config": {
        "vendor-dir" : "setup\/setup_packages\/"
    }

}

